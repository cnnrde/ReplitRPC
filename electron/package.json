{
  "name": "electron",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "make:linux": "electron-forge make --arch arm64 --platform linux",
    "make:windows": "electron-forge make --arch arm64 --platform win32",
    "make:mac:arm64": "electron-forge make --arch arm64 --platform darwin",
    "make:mac:x64": "electron-forge make --arch x64 --platform darwin",
    "make:mac": "yarn make:mac:x64 && yarn make:mac:arm64",
    "release": "yarn make"
  },
  "devDependencies": {
    "@electron-forge/cli": "latest",
    "electron": "^15.1.2",
    "nodemon": "^2.0.13"
  },
  "dependencies": {
    "@electron-forge/maker-pkg": "latest",
    "@koa/cors": "^3.1.0",
    "discord-rpc": "^4.0.1",
    "electron-forge": "^5.2.4",
    "koa": "^2.13.3",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1"
  }
}
